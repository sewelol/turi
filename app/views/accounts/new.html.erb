<%= form_for(@account) do |f| %>
	<p>
		<%= f.label :username %><br />
		<%= f.text_field :username %>
	</p>
	<p>
		<%= f.label :email %><br />
		<%= f.text_field :email %>
	</p>
	<p>
		<%= f.label :password %><br />
		<%= f.password_field :password %>
	</p>
	<p>
		<%= f.label :password_confirmation, "Confirm password" %><br />
		<%= f.password_field :password_confirmation %>
	</p>
	<%= f.submit %>
<% end %>